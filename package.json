{
  "name": "comment-app",
  "author": "tranthithuhan",
  "homepage": "https://tranthithuhan.github.io/comment-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-sass start-js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-sass": "node-sass-chokidar src/ -o src/",
    "watch-sass": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "antd": "^3.23.6",
    "connected-react-router": "^6.5.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.0",
    "gh-pages": "^2.1.1",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.2",
    "nanoid": "^2.1.2",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-enzyme-context": "^1.2.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-auth-wrapper": "^2.1.0",
    "redux-thunk": "^2.3.0"
